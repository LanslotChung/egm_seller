/* components/topTitle/topTitle.wxss */
.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 90rpx;
  background: #3b7ff0;
  z-index: 99999;
  display: flex;
  align-items: center;
  justify-content: center;
}
.custom cover-view{
  color: #fff;
  font-size: 34rpx;
  font-weight: 500;
  max-width: 280rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.goback{
  position: absolute;
  left: 0;
  font-size: 50rpx;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 20rpx;
}
.goback .iconfanhui{
  height: 90rpx;
  line-height: 90rpx;
}
.goback cover-image{
  width: 40rpx;
  height: 40rpx;
  padding: 25rpx 0;
}
.empty_custom{
  height: 90rpx;
  width: 100%;
}
.custom .entering{
  position: absolute;
  left: 40rpx;
  width: 96rpx;
  height: 50rpx;
  text-align: center;
  color: #fff;
  line-height: 50rpx;
  border-radius: 8rpx;
  background-color: #f59a23;
  font-size: 24rpx;
}

